{
  "name": "typescript-module-starter",
  "version": "1.0.0",
  "description": "Simple typescript starter for personal projects.",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [ "lib/" ],
  "scripts": {
    "build": "tsc",
    "build:web": "webpack",
    "test": "jest",
    "lint": "tslint src/**/*.ts __tests__/**/*.ts",
    "docs": "npm run docs:html",
    "docs:html": "typedoc src/ --target ES6 --mode file --out docs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ts",
    "module",
    "starter",
    "tsdoc",
    "webbuild",
    "jest",
    "tslint"
  ],
  "author": "Yengas",
  "license": "ISC",
  "devDependencies": {
    "jest": "^23.3.0",
    "ts-jest": "^23.0.0",
    "ts-loader": "^4.4.2",
    "tslint": "^5.10.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testMatch": [
      "**/__tests__/**/*.+(ts|js)"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  },
  "dependencies": {}
}
